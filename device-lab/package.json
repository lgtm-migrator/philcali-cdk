    "aws-cdk-lib": "2.17.0",
    "jest": "^26.4.2",
    "jsii": "^1.52.1",
    "jsii-pacmak": "^1.52.1",
    "constructs": "10.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "~3.9.7"
